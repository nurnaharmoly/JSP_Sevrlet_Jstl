CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ITEM(ID VARCHAR(255) NOT NULL PRIMARY KEY,TITLE VARCHAR(255) NOT NULL,LINK VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255) NOT NULL,CONTENT VARCHAR(255) NOT NULL,TIME DATE NOT NULL,NEWSFEED_ID VARCHAR(255))
CREATE MEMORY TABLE NEWSFEED(ID VARCHAR(255) NOT NULL PRIMARY KEY,URL VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_361 UNIQUE(URL))
CREATE MEMORY TABLE SUBSCRIPTION(ID VARCHAR(255) NOT NULL PRIMARY KEY,NAME VARCHAR(255) NOT NULL,NEWSFEED_ID VARCHAR(255),USER_ID VARCHAR(255),CONSTRAINT FK1456591D75D72ACF FOREIGN KEY(NEWSFEED_ID) REFERENCES NEWSFEED(ID))
CREATE MEMORY TABLE USER(ID VARCHAR(255) NOT NULL PRIMARY KEY,NAME VARCHAR(255) NOT NULL,CONSTRAINT SYS_CT_365 UNIQUE(NAME))
ALTER TABLE ITEM ADD CONSTRAINT FK317B1375D72ACF FOREIGN KEY(NEWSFEED_ID) REFERENCES NEWSFEED(ID)
ALTER TABLE SUBSCRIPTION ADD CONSTRAINT FK1456591D7038670F FOREIGN KEY(USER_ID) REFERENCES USER(ID)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
INSERT INTO ITEM VALUES('402880e5082c912101082c9188800003','Survey: Interest in MP3 Players Grows (AP)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/ap/20051215/ap_on_hi_te/digital_devices','<p><a href="http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/ap/20051215/ap_on_hi_te/digital_devices"><img src="http://us.news3.yimg.com/us.i2.yimg.com/p/rids/20051121/i/r200905222.jpg?x=95&amp;y=130&amp;sig=mYRTP3k91kf7jk7n2uqLBw--" align="left" height="130" width="95" alt="An Apple Computer Inc. employee holds the new digital music player ''iPod Nano'' at a news conference in Tokyo September 8, 2005. Apple Computer Inc. on Monday said it would prepay $1.25 billion to secure its supply of flash memory used in its market-dominating iPod music players amid an expected surge in demand for the chips. REUTERS/Toru Hanai" border="0" /></a>AP - Shutterbugs, time to face the music. Portable digital music players are now on par with digital cameras as the most wanted electronics items, a new survey finds.</p><br clear=all>','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c91889f0004','Intel Unveils New Notebook PC Platform (NewsFactor)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nf/20051214/tc_nf/40197','NewsFactor - Intel is readying a major update to the Centrino platform. The refresh, codenamed Napa, includes the Yonah dual-core processor, a new motherboard chipset, and an updated wireless package.','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c9188bf0005','MTV, Microsoft Set To Take on Apple''s iTunes (NewsFactor)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nf/20051214/tc_nf/40175','NewsFactor - Microsoft and Viacom''s MTV will join forces to develop a digital-music service that relies on the next version of Windows Media Player.','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c9188ed0006','The High-Speed Cellular-Network Showdown (NewsFactor)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nf/20051214/tc_nf/39872','NewsFactor - Voice quality has long been the primary concern of most mobile-phone users. But with a rise in the popularity of advanced smartphones as well as new interest in receiving audio and video streams on mobile devices, speed is likely to become a critical issue for those deciding which wireless provider to pick.','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c91894b0007','Review: Averatec 3715-EH1 (NewsFactor)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nf/20051214/tc_nf/40080','NewsFactor - Even with a focus on low-cost, thin-and-light notebooks, Averatec packs many features into its systems, giving customers the best bang for their buck. The company''s latest product, the Averatec 3715-EH1, offers an AMD-based, travel-friendly laptop with decent amenities at a reasonable price.','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c91897a0008','Taiwan firm chosen for 100-dollar laptop program (AFP)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/afp/20051214/tc_afp/usitcomputerlaptop','<p><a href="http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/afp/20051214/tc_afp/usitcomputerlaptop"><img src="http://us.news3.yimg.com/us.i2.yimg.com/p/afp/20051214/capt.sge.oee57.141205203623.photo00.photo.default-370x278.jpg?x=130&amp;y=97&amp;sig=XiPu3DFE9mkSO4tOFSqYIA--" align="left" height="97" width="130" alt="This artist rendition released by the Massachusetts Intstitute of Technology shows the proposed 100 USD laptop being developed by MIT engineers. Taiwan''s Quanta Computer has been chosen to make a 100-dollar laptop computer developed at MIT for distribution to millions of children in developing countries.(AFP/MIT-HO/File)" border="0" /></a>AFP - Taiwan&#039;s Quanta Computer has been chosen to make a 100-dollar laptop computer developed at MIT for distribution to millions of children in developing countries.</p><br clear=all>','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c918a930009','iPod and MP3 players top US Christmas wish lists (AFP)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/afp/20051214/tc_afp/afplifestyleconsumer','<p><a href="http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/afp/20051214/tc_afp/afplifestyleconsumer"><img src="http://us.news3.yimg.com/us.i2.yimg.com/p/afp/20051214/capt.sge.oee94.141205203900.photo00.photo.default-384x270.jpg?x=130&amp;y=91&amp;sig=uewBsBMgpCZIDYg6L0oidA--" align="left" height="91" width="130" alt="A South Korean promoter shows the new iPod shuffle. Santa''s sack will be bulging with iPods and other trendy digital audio devices, according to a new survey of America''s Christmas wish lists.(AFP/File/Jung Yeon-Je)" border="0" /></a>AFP - Santa&#039;s sack will be bulging with iPods and other trendy digital audio devices, according to a new survey of America&#039;s Christmas wish lists.</p><br clear=all>','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c918ab3000a','Video game-related sales fall in November (Reuters)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nm/20051214/tc_nm/media_videogames_dc','<p><a href="http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nm/20051214/tc_nm/media_videogames_dc"><img src="http://us.news3.yimg.com/us.i2.yimg.com/p/nm/20051214/2005_12_13t153918_450x341_us_media_videogames.jpg?x=130&amp;y=98&amp;sig=alm.mh8rZ.vLfO7c6OvDWA--" align="left" height="98" width="130" alt="Microsoft Corps Corporate Vice President of Worldwide Marketing and Publishing Peter Moore (L) hands the companys new Xbox 360 video game console to a Japanese customer in Tokyo December 10, 2005. Microsoft Corp. has worked to keep retail supply expectations low for its new Xbox 360 video game console and is not likely to deliver more units than expected before Christmas, an analyst said on Tuesday. (Yuriko Nakao/Reuters)" border="0" /></a>Reuters - U.S. retail sales of video game \u000ahardware and software fell 9 percent from a year ago to come in \u000ajust under &#36;1.3 billion, market researchers NPD Group said on \u000aWednesday.</p><br clear=all>','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c918ae1000b','Intel touts power of new laptop chip (Reuters)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nm/20051214/tc_nm/intel_centrino_dc','<p><a href="http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/nm/20051214/tc_nm/intel_centrino_dc"><img src="http://us.news3.yimg.com/us.i2.yimg.com/p/nm/20051214/2005_12_13t215029_450x278_us_intel_centrino.jpg?x=130&amp;y=80&amp;sig=NSMw75d8dUqJ2XH6qVFE8w--" align="left" height="80" width="130" alt="Craig Barrett speaks at an event to launch Intels new Centrino Mobile Technology in New York City, March 12, 2003. Intel Corp. said on Tuesday its next-generation platform for laptop computers will use 25 percent less power. The overhaul of Intel''s Centrino technology, code-named ''Napa,'' comes as the world''s largest chipmaker is getting a boost from laptop sales. (Mike Segar/Reuters)" border="0" /></a>Reuters - Intel Corp.  said on \u000aTuesday its next-generation platform for laptop computers will \u000ause 25 percent less power while boosting performance, helping \u000ait stay on top of the fast-growing market.</p><br clear=all>','','2005-12-14','402880e5082c912101082c91650a0001')
INSERT INTO ITEM VALUES('402880e5082c912101082c918c58000c','Video Games Have Sequel-Happy Year (AP)','http://us.rd.yahoo.com/dailynews/rss/personaltech/*http://news.yahoo.com/s/ap/20051214/ap_on_hi_te/ye_video_games_ye6a','AP - Beyond a few innovative titles and some promising new hardware, the video game industry largely continued to do what it does best in 2005: churning out sequels, movie tie-ins and ultraviolence.','','2005-12-14','402880e5082c912101082c91650a0001')
